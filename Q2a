
def square_list(input_list):
    return [item ** 2 for item in input_list]


number = [1, 2, 3, 4, 5, 6, 7]
expected_output = [1, 4, 9, 16, 25, 36, 49]
squared_result = square_list(number)
print("Task 1: Squared list:", squared_result)
print("Expected output:", expected_output)


def concatenate_lists(list1, list2):
   
    if len(list1) != len(list2):
        return "Lists must have the same length for index-wise concatenation."
    
    
    result = []
    
    
    for i in range(len(list1)):
        result.append(list1[i] + list2[i])
    
    return result


list1 = ["Xin", "cam"]
list2 = ["chao", "on"]
concatenated_result = concatenate_lists(list1, list2)
print("Task 2: Concatenated lists:", concatenated_result)
